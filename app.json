{
  "expo": {
    "name": "react-native-graph",
    "slug": "react-native-graph",
    "assetBundlePatterns": ["**/*"],
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "react-native-graph",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", ["expo-font",        
    {
      "fonts": ["./assets/fonts/*"]
    }]],
    "experiments": {
      "typedRoutes": true
    }
  }
}
